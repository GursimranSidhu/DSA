#include<iostream>
using namespace std;
int main()
{
    int n;
    cout<<"Enter the number of elements you want: ";
    cin>>n;
    int sum=0;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cout<<"Enter the "<<i<<" element of array: ";
        cin>>arr[i];
    }
    for(int i=0;i<n;i++)
    {
        sum+=arr[i];
    }
    cout<<"Sum of elements in array is "<<sum;
    return 0;
}
